{
  "$schema": "https://turborepo.com/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*", "apps/**/.env*"],
      "outputs": [".vercel/**", ".next/**", "!.next/cache/**", "dist/**", "assets/**"],
      "env": [
        "NEXT_PUBLIC_CLERK_PUBLISHABLE_KEY",
        "CLERK_SECRET_KEY",
        "NEXT_PUBLIC_APP_NAME",
        "NEXT_PUBLIC_BASE_URL",
        "NEXT_PUBLIC_API_URL",
        "SUPABASE_SECRET_KEY",
        "SUPABASE_BUCKET",
        "NEXT_PUBLIC_SUPABASE_PUBLISHABLE_KEY",
        "EAS_WEBHOOK_SECRET",
        "DATABASE_URL",
        "DIRECT_URL",
        "SALT",
        "JWT_ACCESS_SECRET",
        "JWT_REFRESH_SECRET",
        "ACESS_TOKEN_TTL",
        "REFRESH_TOKEN_TTL",
        "NODE_ENV"
      ]
    },
    "lint": {
      "dependsOn": ["^lint"],
      "outputs": []
    },
    "check-types": {
      "dependsOn": ["^check-types"]
    },
    "generate": {
      "dependsOn": ["^generate"],
      "outputs": ["assets/**"],
      "cache": false,
      "persistent": false
    },
    "dev": {
      "cache": false,
      "persistent": true
    }
  }
}
