{
  "name": "mono-kot-jingrwai",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lynti:dev": "turbo run dev --filter web --filter lynti-bneng --filter @repo/ui-native",
    "mobile:dev": "turbo run dev --filter web --filter kot-ong-rwai --filter @repo/ui-native",
    "clean:install": "node scripts/clean-install.mjs",
    "generate": "turbo run generate",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/react": "~19.0.10",
    "@types/react-dom": "~19.2.3",
    "eslint": "^9.39.2",
    "globals": "^16.5.0",
    "prettier": "^3.7.4",
    "turbo": "^2.7.1",
    "typescript": "5.8.3",
    "typescript-eslint": "^8.50.0"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18"
  },
  "pnpm": {
    "overrides": {
      "nativewind": "~4.1.23",
      "react-native-reanimated": "~3.17.4",
      "react-native-safe-area-context": "5.4.0",
      "@types/react": "19.0.10",
      "react": "19.0.0",
      "react-dom": "19.0.0",
      "react-native": "0.79.6",
      "metro": "^0.82.0"
    },
    "public-hoist-pattern": [
      "react",
      "react-dom",
      "react-native"
    ],
    "shamefully-hoist": true
  }
}
