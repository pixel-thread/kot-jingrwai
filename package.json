{
  "name": "mono-kot-jingrwai",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "preinstall": "node scripts/enforce-expo-versions.js",
    "clean:install": "node scripts/clean-install.mjs",
    "generate": "turbo run generate",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "pnpm exec eslint --fix"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/react": "~19.0.10",
    "@types/react-dom": "~19.2.3",
    "eslint": "^9.39.2",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.7.4",
    "turbo": "^2.7.1",
    "typescript": "5.8.3",
    "typescript-eslint": "^8.50.0"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18"
  },
  "pnpm": {
    "overrides": {
      "nativewind": "~4.1.23",
      "react-native-reanimated": "~3.17.4",
      "react-native-safe-area-context": "5.4.0",
      "@types/react": "~19.0.10",
      "@types/react-dom": "~19.2.3"
    },
    "shamefully-hoist": true
  }
}
