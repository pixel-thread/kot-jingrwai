{
  "name": "kot-ong-rwai",
  "version": "1.0.1",
  "main": "expo-router/entry",
  "scripts": {
    "android": "expo run:android",
    "ios": "expo run:ios",
    "start": "expo start",
    "dev": "expo start --clear --port 8081",
    "prebuild": "expo prebuild",
    "fetchSongs": "node scripts/fetch-songs.js",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\" && prettier -c \"**/*.{js,jsx,ts,tsx,json}\"",
    "format": "eslint \"**/*.{js,jsx,ts,tsx}\" --fix && prettier \"**/*.{js,jsx,ts,tsx,json}\" --write",
    "web": "expo start --web",
    "local:dev:android": "eas build --profile development --platform android --local",
    "local:preview:android": "eas build --profile preview --platform android --local",
    "local:prod:android": "eas build --profile production --platform android --local",
    "build:dev": "eas build --profile development --platform all",
    "build:preview": "eas build --profile preview --platform all",
    "build:prod": "eas build --profile production --platform all",
    "build:ios:dev": "eas build --profile development --platform ios",
    "build:ios:preview": "eas build --profile preview --platform ios",
    "build:ios:prod": "eas build --profile production --platform ios",
    "build:android:dev": "eas build --profile development --platform android",
    "build:android:preview": "eas build --profile preview --platform android",
    "build:android:prod": "eas build --profile production --platform android",
    "update:preview": "eas update --branch preview --message \"Preview update\"",
    "update:prod": "eas update --branch production --message \"Production update\""
  },
  "dependencies": {
    "@repo/constants": "workspace:*",
    "@repo/utils": "workspace:*",
    "@repo/ui-native": "workspace:*",
    "@repo/libs": "workspace:*",
    "@expo/vector-icons": "^14.0.0",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-navigation/drawer": "^7.3.9",
    "@react-navigation/native": "^7.0.3",
    "@shopify/flash-list": "1.7.6",
    "@tanstack/react-query": "^5.81.5",
    "axios": "^1.9.0",
    "babel-plugin-inline-import": "^3.0.0",
    "expo": "~53.0.24",
    "expo-audio": "~0.4.9",
    "expo-clipboard": "~7.1.5",
    "expo-constants": "~17.1.4",
    "expo-font": "~13.3.2",
    "expo-haptics": "~14.1.4",
    "expo-keep-awake": "~14.1.4",
    "expo-linking": "~7.1.7",
    "expo-network": "~7.1.5",
    "expo-router": "~5.1.8",
    "expo-splash-screen": "~0.30.10",
    "expo-status-bar": "~2.2.3",
    "expo-system-ui": "~5.0.11",
    "expo-updates": "~0.28.17",
    "expo-web-browser": "~14.2.0",
    "nativewind": "4.1.23",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.6",
    "react-native-css-interop": "^0.2.1",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-get-random-values": "^1.11.0",
    "react-native-reanimated": "~3.17.5",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-uuid": "^2.0.3",
    "react-native-web": "^0.20.0",
    "react-native-worklets-core": "^1.6.2",
    "zustand": "^4.5.7"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@repo/types": "workspace:*",
    "@babel/core": "^7.20.0",
    "@types/react": "~19.0.10",
    "add": "^2.0.6",
    "ajv": "^8.12.0",
    "eslint": "^9.25.1",
    "eslint-config-expo": "^9.2.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-unused-imports": "^4.1.4",
    "or": "^0.2.0",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "tailwindcss": "^3.4.0"
  },
  "private": true
}
