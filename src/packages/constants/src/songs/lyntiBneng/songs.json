[
  {
    "id": "song-1-1",
    "title": "Ka pyrthei baroh",
    "metadata": {
      "id": "metadata-1-1",
      "number": 1,
      "oldNumber": null,
      "language": "khasi",
      "author": "N/A",
      "composer": "N/A",
      "isChorus": false,
      "tags": "N/A",
      "syllables": null,
      "reference": "N/A",
      "tune": "N/A",
      "meter": "N/A",
      "songId": "song-1-1"
    },
    "paragraphs": [
      {
        "id": "para-1-1",
        "order": 1,
        "type": "VERSE",
        "songId": "song-1-1",
        "lines": [
          {
            "id": "line-1-1-1",
            "order": 0,
            "text": "Ka pyrthei baroh ka la khmih lynti,"
          },
          {
            "id": "line-1-1-2",
            "order": 1,
            "text": "Ki bahok la slem ki la kwah la sliang,"
          },
          {
            "id": "line-1-1-3",
            "order": 2,
            "text": "Ba kan duh ka miet, ba kan mih ka sngi."
          },
          {
            "id": "line-1-1-4",
            "order": 3,
            "text": "U Jisu Un wan, ngin dup ioh u Ksiang."
          }
        ]
      },
      {
        "id": "para-1-2",
        "order": 2,
        "type": "VERSE",
        "songId": "song-1-1",
        "lines": [
          {
            "id": "line-1-2-1",
            "order": 0,
            "text": "Ka mynsiem ka dum ha kata ka miet,"
          },
          {
            "id": "line-1-2-2",
            "order": 1,
            "text": "Ka dohnud ruh kem da ka jingbymman:"
          },
          {
            "id": "line-1-2-3",
            "order": 2,
            "text": "Kiba ngeit kumta ki ïapleit ïa khmied,"
          },
          {
            "id": "line-1-2-4",
            "order": 3,
            "text": "Ba Un hiar u Khrist, u Khun Blei Un wan."
          }
        ]
      },
      {
        "id": "para-1-3",
        "order": 3,
        "type": "VERSE",
        "songId": "song-1-1",
        "lines": [
          {
            "id": "line-1-3-1",
            "order": 0,
            "text": "Te ha khmih shisha, la jan poi ka ïa,"
          },
          {
            "id": "line-1-3-2",
            "order": 1,
            "text": "Ka jingwan u Khrist, ka jingsuk u briew;"
          },
          {
            "id": "line-1-3-3",
            "order": 2,
            "text": "Ko riewngeit, to kmen, phin sa ïoh jingbha."
          },
          {
            "id": "line-1-3-4",
            "order": 3,
            "text": "U la jan u Khrist, U la wan ban siew."
          }
        ]
      }
    ],
    "metadataId": "metadata-1-1",
    "trackId": null,
    "track": null
  },
  {
    "id": "song-1-2",
    "title": "Ale, ko Jingstad",
    "metadata": {
      "id": "metadata-1-2",
      "number": 2,
      "oldNumber": null,
      "language": "khasi",
      "author": "N/A",
      "composer": "N/A",
      "isChorus": false,
      "tags": "N/A",
      "syllables": null,
      "reference": "N/A",
      "tune": "N/A",
      "meter": "N/A",
      "songId": "song-1-2"
    },
    "paragraphs": [
      {
        "id": "para-2-1",
        "order": 1,
        "type": "VERSE",
        "songId": "song-1-2",
        "lines": [
          {
            "id": "line-2-1-1",
            "order": 0,
            "text": "Ale, ko Jingstad bymjukut,"
          },
          {
            "id": "line-2-1-2",
            "order": 1,
            "text": "Ale, to pynbeit ki jingmut;"
          },
          {
            "id": "line-2-1-3",
            "order": 2,
            "text": "To wan pynhikai ïa ka hok,"
          },
          {
            "id": "line-2-1-4",
            "order": 3,
            "text": "Ba ïoh ngin sa leit sha dujok."
          },
          {
            "id": "line-2-1-5",
            "order": 4,
            "text": "To kmen, to kmen, ko Israel,"
          },
          {
            "id": "line-2-1-6",
            "order": 5,
            "text": "La jan wan u Emanuel."
          }
        ]
      },
      {
        "id": "para-2-2",
        "order": 2,
        "type": "VERSE",
        "songId": "song-1-2",
        "lines": [
          {
            "id": "line-2-2-1",
            "order": 0,
            "text": "Ko Syiem bakhraw tam ki bynriew"
          },
          {
            "id": "line-2-2-2",
            "order": 1,
            "text": "Ale, ba Men jop ka jingsniew;"
          },
          {
            "id": "line-2-2-3",
            "order": 2,
            "text": "To wan ban pynbha ki bajot,"
          },
          {
            "id": "line-2-2-4",
            "order": 3,
            "text": "To wan ïakyrshan ki batlot."
          }
        ]
      },
      {
        "id": "para-2-3",
        "order": 3,
        "type": "VERSE",
        "songId": "song-1-2",
        "lines": [
          {
            "id": "line-2-3-1",
            "order": 0,
            "text": "A Emanuel badonbor,"
          },
          {
            "id": "line-2-3-2",
            "order": 1,
            "text": "Pynim ki mynsiem bakordor;"
          },
          {
            "id": "line-2-3-3",
            "order": 2,
            "text": "Wat shah ba kin duh ha ka pop,"
          },
          {
            "id": "line-2-3-4",
            "order": 3,
            "text": "A Trai Jisu Khrist, to wan jop."
          }
        ]
      }
    ],
    "metadataId": "metadata-1-2",
    "trackId": null,
    "track": null
  },
  {
    "id": "song-1-3",
    "title": "Pynjaw, ko bneng",
    "metadata": {
      "id": "metadata-1-3",
      "number": 3,
      "oldNumber": null,
      "language": "khasi",
      "author": "N/A",
      "composer": "N/A",
      "isChorus": false,
      "tags": "N/A",
      "syllables": null,
      "reference": "Is. 45:8",
      "tune": "N/A",
      "meter": "N/A",
      "songId": "song-1-3"
    },
    "paragraphs": [
      {
        "id": "para-3-0",
        "order": 0,
        "type": "INTRO",
        "songId": "song-1-3",
        "lines": [
          {
            "id": "line-3-0-1",
            "order": 0,
            "text": "Pynjaw, ko bneng,"
          },
          {
            "id": "line-3-0-2",
            "order": 1,
            "text": "Na suiñ to pynslap ï’u Bahok;"
          },
          {
            "id": "line-3-0-3",
            "order": 2,
            "text": "To ang, khyndew,"
          },
          {
            "id": "line-3-0-4",
            "order": 3,
            "text": "Pynmih, seiñlung ï’u Nongpynim."
          }
        ]
      },
      {
        "id": "para-3-1",
        "order": 1,
        "type": "VERSE",
        "songId": "song-1-3",
        "lines": [
          {
            "id": "line-3-1-1",
            "order": 0,
            "text": "Ko Trai, wat bitar ïa ka pop;"
          },
          {
            "id": "line-3-1-2",
            "order": 1,
            "text": "Ko Trai, wat kynmaw jingbymhok;"
          },
          {
            "id": "line-3-1-3",
            "order": 2,
            "text": "U paid ba Me jied bad Me tim,"
          },
          {
            "id": "line-3-1-4",
            "order": 3,
            "text": "Jngai na Me u la poi sha jingjot."
          }
        ]
      },
      {
        "id": "para-3-2",
        "order": 2,
        "type": "VERSE",
        "songId": "song-1-3",
        "lines": [
          {
            "id": "line-3-2-1",
            "order": 0,
            "text": "Kumba rah sla dieng ka lyer tlang,"
          },
          {
            "id": "line-3-2-2",
            "order": 1,
            "text": "Kumta sa ïa ngi rah ka pop;"
          },
          {
            "id": "line-3-2-3",
            "order": 2,
            "text": "Ko Trai, Me phai dien la ka khmat,"
          },
          {
            "id": "line-3-2-4",
            "order": 3,
            "text": "Bad Me rai pynsakma ki khun jot."
          }
        ]
      },
      {
        "id": "para-3-3",
        "order": 3,
        "type": "VERSE",
        "songId": "song-1-3",
        "lines": [
          {
            "id": "line-3-3-1",
            "order": 0,
            "text": "To dem, to peit, sngap isynei"
          },
          {
            "id": "line-3-3-2",
            "order": 1,
            "text": "L’u paid ba lyngïar khtang, ko Blei;"
          },
          {
            "id": "line-3-3-3",
            "order": 2,
            "text": "I’u Syiem b’la kular to phah law"
          },
          {
            "id": "line-3-3-4",
            "order": 3,
            "text": "Ka jingkhun ba ïa ngi ka teh mraw."
          }
        ]
      },
      {
        "id": "para-3-4",
        "order": 4,
        "type": "VERSE",
        "songId": "song-1-3",
        "lines": [
          {
            "id": "line-3-4-1",
            "order": 0,
            "text": "“To tngen bad to kmen, paid jong Nga,"
          },
          {
            "id": "line-3-4-2",
            "order": 1,
            "text": "Jingmap bad jingsuk ki la jan;"
          },
          {
            "id": "line-3-4-3",
            "order": 2,
            "text": "Wat tieng, Ngan pynkoit, Ngan pyllait:"
          },
          {
            "id": "line-3-4-4",
            "order": 3,
            "text": "Nga dei u Nongpynim ban sa wan.”"
          }
        ]
      }
    ],
    "metadataId": "metadata-1-3",
    "trackId": null,
    "track": null
  },
  {
    "id": "song-1-4",
    "title": "To wan, to wan, Emanuel",
    "metadata": {
      "id": "metadata-1-4",
      "number": 4,
      "oldNumber": null,
      "language": "khasi",
      "author": "N/A",
      "composer": "N/A",
      "isChorus": false,
      "tags": "N/A",
      "syllables": null,
      "reference": "N/A",
      "tune": "N/A",
      "meter": "N/A",
      "songId": "song-1-4"
    },
    "paragraphs": [
      {
        "id": "para-4-1",
        "order": 1,
        "type": "VERSE",
        "songId": "song-1-4",
        "lines": [
          { "id": "line-4-1-1", "order": 0, "text": "To wan, to wan, Emanuel," },
          { "id": "line-4-1-2", "order": 1, "text": "To wan siewspah ï’u Israel:" },
          { "id": "line-4-1-3", "order": 2, "text": "Siewspah bynriew ba Me la thaw," },
          { "id": "line-4-1-4", "order": 3, "text": "Ba ud rangli ha jinglong mraw." },
          { "id": "line-4-1-5", "order": 4, "text": "To kmen! To kmen, ko Israel!" },
          { "id": "line-4-1-6", "order": 5, "text": "U la jan wan Emanuel!" }
        ]
      },
      {
        "id": "para-4-2",
        "order": 2,
        "type": "VERSE",
        "songId": "song-1-4",
        "lines": [
          { "id": "line-4-2-1", "order": 0, "text": "To wan, to wan, Jingstad u Blei," },
          { "id": "line-4-2-2", "order": 1, "text": "Wan pynrnythi ïa ka pyrthei," },
          { "id": "line-4-2-3", "order": 2, "text": "B’la jot sakma ynda la ngop" },
          { "id": "line-4-2-4", "order": 3, "text": "Bad ngat d’u kusid ha riam ka pop." },
          { "id": "line-4-2-5", "order": 4, "text": "To kmen! To kmen, ko Israel!" },
          { "id": "line-4-2-6", "order": 5, "text": "U la jan wan Emanuel!" }
        ]
      },
      {
        "id": "para-4-3",
        "order": 3,
        "type": "VERSE",
        "songId": "song-1-4",
        "lines": [
          { "id": "line-4-3-1", "order": 0, "text": "To wan, Jingkwah ki jaitbynriew," },
          { "id": "line-4-3-2", "order": 1, "text": "Pyniãtlli jingmut ki briew;" },
          { "id": "line-4-3-3", "order": 2, "text": "Pyndam ka nia bishni bymdei," },
          { "id": "line-4-3-4", "order": 3, "text": "Wan rah jingsuk ha ka pyrthei." },
          { "id": "line-4-3-5", "order": 4, "text": "To kmen! To kmen, ko Israel!" },
          { "id": "line-4-3-6", "order": 5, "text": "U la jan wan Emanuel!" }
        ]
      },
      {
        "id": "para-4-4",
        "order": 4,
        "type": "VERSE",
        "songId": "song-1-4",
        "lines": [
          { "id": "line-4-4-1", "order": 0, "text": "To wan, Jingshai ka bymjukut," },
          { "id": "line-4-4-2", "order": 1, "text": "Ko Sngi ka hok, Jingstad khlem kut;" },
          { "id": "line-4-4-3", "order": 2, "text": "Pynshai kiba ïa-im thala," },
          { "id": "line-4-4-4", "order": 3, "text": "Kiba tare ha jingbakla." },
          { "id": "line-4-4-5", "order": 4, "text": "To kmen! To kmen, ko Israel!" },
          { "id": "line-4-4-6", "order": 5, "text": "U la jan wan Emanuel!" }
        ]
      },
      {
        "id": "para-4-5",
        "order": 5,
        "type": "VERSE",
        "songId": "song-1-4",
        "lines": [
          { "id": "line-4-5-1", "order": 0, "text": "To wan, to wan, ko Nongpynim:" },
          { "id": "line-4-5-2", "order": 1, "text": "Bynriew is’nei, wan pyllait-im;" },
          { "id": "line-4-5-3", "order": 2, "text": "Shah set byndi, nongpop b’la rem," },
          { "id": "line-4-5-4", "order": 3, "text": "A wan pyllait, to wat pynslem." },
          { "id": "line-4-5-5", "order": 4, "text": "To kmen! To kmen, ko Israel!" },
          { "id": "line-4-5-6", "order": 5, "text": "U la jan wan Emanuel!" }
        ]
      }
    ],
    "metadataId": "metadata-1-4",
    "trackId": null,
    "track": null
  },
  {
    "id": "song-1-7",
    "title": "Ko Khrist Jisu, to wan noh kloi",
    "metadata": {
      "id": "metadata-1-7",
      "number": 7,
      "oldNumber": null,
      "language": "khasi",
      "author": "N/A",
      "composer": "N/A",
      "isChorus": false,
      "tags": "N/A",
      "syllables": null,
      "reference": "N/A",
      "tune": "N/A",
      "meter": "N/A",
      "songId": "song-1-7"
    },
    "paragraphs": [
      {
        "id": "para-7-1",
        "order": 1,
        "type": "VERSE",
        "songId": "song-1-7",
        "lines": [
          { "id": "line-7-1-1", "order": 0, "text": "Ko Khrist Jisu, to wan noh kloi," },
          { "id": "line-7-1-2", "order": 1, "text": "To wan wat pynslem shuh;" },
          { "id": "line-7-1-3", "order": 2, "text": "Bynriew baroh u ap lynta," },
          { "id": "line-7-1-4", "order": 3, "text": "Ïa sngi jingwan jong Me." }
        ]
      }
    ],
    "metadataId": "metadata-1-7",
    "trackId": null,
    "track": null
  },
  {
    "id": "song-1-8",
    "title": "Wan, Jisu wat ym pynslem",
    "metadata": {
      "id": "metadata-1-8",
      "number": 8,
      "oldNumber": null,
      "language": "khasi",
      "author": "N/A",
      "composer": "N/A",
      "isChorus": false,
      "tags": "N/A",
      "syllables": null,
      "reference": "N/A",
      "tune": "N/A",
      "meter": "N/A",
      "songId": "song-1-8"
    },
    "paragraphs": [
      {
        "id": "para-8-1",
        "order": 1,
        "type": "VERSE",
        "songId": "song-1-8",
        "lines": [
          { "id": "line-8-1-1", "order": 0, "text": "Wan, Jisu wat ym pynslem," },
          { "id": "line-8-1-2", "order": 1, "text": "Wan, pyllait ïa ngi na shlem;" },
          { "id": "line-8-1-3", "order": 2, "text": "Na jingïap ki pop pynim," },
          { "id": "line-8-1-4", "order": 3, "text": "Tang ha Me jingthngen ngin shem." }
        ]
      },
      {
        "id": "para-8-chorus",
        "order": 2,
        "type": "CHORUS",
        "songId": "song-1-8",
        "lines": [
          { "id": "line-8-c-1", "order": 0, "text": "To tied sngewthiang baje basyiang," },
          { "id": "line-8-c-2", "order": 1, "text": "Ban pdiang burom ï’u Syiem Bathiang." },
          { "id": "line-8-c-3", "order": 2, "text": "To tied sngewthiang ha ki saw lyer," },
          { "id": "line-8-c-4", "order": 3, "text": "Ban pdiang burom ï’u Syiem." }
        ]
      },
      {
        "id": "para-8-2",
        "order": 3,
        "type": "VERSE",
        "songId": "song-1-8",
        "lines": [
          { "id": "line-8-2-1", "order": 0, "text": "Ka jingtngen jong u bynriew" },
          { "id": "line-8-2-2", "order": 1, "text": "Jingkyrmen jong u Me dei" },
          { "id": "line-8-2-3", "order": 2, "text": "Jingangnud ki ithma," },
          { "id": "line-8-2-4", "order": 3, "text": "Bad jingkmen jong ki shirtya." }
        ]
      },
      {
        "id": "para-8-3",
        "order": 4,
        "type": "VERSE",
        "songId": "song-1-8",
        "lines": [
          { "id": "line-8-3-1", "order": 0, "text": "Ban siewspah ïa ka pyrthei" },
          { "id": "line-8-3-2", "order": 1, "text": "Me wan kha kum u Khunlung;" },
          { "id": "line-8-3-3", "order": 2, "text": "U Jait Syiem pynban Me dei," },
          { "id": "line-8-3-4", "order": 3, "text": "To wanlam hima jong Me." }
        ]
      },
      {
        "id": "para-8-4",
        "order": 5,
        "type": "VERSE",
        "songId": "song-1-8",
        "lines": [
          { "id": "line-8-4-1", "order": 0, "text": "D’u Mynsiem Bakhuid synshar" },
          { "id": "line-8-4-2", "order": 1, "text": "Ha dohnud bun salonsar;" },
          { "id": "line-8-4-3", "order": 2, "text": "Da jingdap ka khet burom," },
          { "id": "line-8-4-4", "order": 3, "text": "To kyntiew ïa ki sha jrong." }
        ]
      }
    ],
    "metadataId": "metadata-1-8",
    "trackId": null,
    "track": null
  },
  {
    "id": "song-1-13",
    "title": "Ka Lynnong Rit",
    "metadata": {
      "id": "metadata-1-13",
      "number": 13,
      "oldNumber": null,
      "language": "khasi",
      "author": "N/A",
      "composer": "N/A",
      "isChorus": false,
      "tags": "N/A",
      "syllables": null,
      "reference": "N/A",
      "tune": "N/A",
      "meter": "N/A",
      "songId": "song-1-13"
    },
    "paragraphs": [
      {
        "id": "para-13-1",
        "order": 1,
        "type": "VERSE",
        "songId": "song-1-13",
        "lines": [
          {
            "id": "line-13-1-1",
            "order": 0,
            "text": "U Nongïalam lynti jong ngi, u Khun langbrot ba khlem thoh dak,"
          },
          {
            "id": "line-13-1-2",
            "order": 1,
            "text": "U rung na ka bynta jong ngi, ba la pynlong kum u Melkhizedek,"
          },
          {
            "id": "line-13-1-3",
            "order": 2,
            "text": "u Lyngdoh sha bymjukut bad junom la junom,"
          },
          {
            "id": "line-13-1-4",
            "order": 3,
            "text": "U long u Syiem jong ka hok, ka pateng jong U kam don jingkut."
          }
        ]
      },
      {
        "id": "para-13-2",
        "order": 2,
        "type": "VERSE",
        "songId": "song-1-13",
        "lines": [
          {
            "id": "line-13-2-1",
            "order": 0,
            "text": "P. – Ka jingsngewnguh ï’u Blei."
          }
        ]
      }
    ],
    "metadataId": "metadata-1-13",
    "trackId": null,
    "track": null
  },
  {
