{
  "name": "@repo/native-ui",
  "version": "0.0.0",
  "private": true,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "default": "./dist/*.js"
    },
    "./*/**": {
      "types": "./dist/*/**/*.d.ts",
      "default": "./dist/*/**/*.js"
    }
  },
  "scripts": {
    "lint": "eslint . --max-warnings 0",
    "generate": "tsc -p tsconfig.json",
    "check-types": "tsc --noEmit"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@types/node": "^22.15.3",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "eslint": "^9.39.1",
    "typescript": "5.9.2"
  },
  "dependencies": {
    "@repo/libs": "workspace:*",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-native": "*"
  }
}
