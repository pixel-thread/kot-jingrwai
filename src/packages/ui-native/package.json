{
  "name": "@repo/ui-native",
  "version": "0.0.0",
  "private": true,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "default": "./dist/*.js"
    },
    "./*/**": {
      "types": "./dist/*/**/*.d.ts",
      "default": "./dist/*/**/*.js"
    }
  },
  "scripts": {
    "generate": "tsc -p tsconfig.json",
    "check-types": "tsc --noEmit",
    "dev": "tsc -w -p tsconfig.json"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@types/node": "^22.15.3",
    "@repo/types": "workspace:*",
    "eslint": "^9.39.1",
    "prettier-plugin-tailwindcss": "^0.5.11"
  },
  "dependencies": {
    "@repo/libs": "workspace:*",
    "@repo/utils": "workspace:*",
    "class-variance-authority": "^0.7.1"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  }
}
